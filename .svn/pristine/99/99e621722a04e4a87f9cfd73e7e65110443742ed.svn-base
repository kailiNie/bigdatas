<div class="category-panel">
    <i class="borad_corner borad_corner_1"></i>
    <i class="borad_corner borad_corner_2"></i>
    <i class="borad_corner borad_corner_3"></i>
    <i class="borad_corner borad_corner_4"></i>

    <ul class="category-list">
        <li class="clearfix" *ngFor="let item of optionData">
            <span class="category-list-name tx-overflow">{{item.title}}</span>
            <span class="category-list-bar-wrap">
                <span class="category-list-bar category-list-bar1"  [ngStyle]="{'width': item.proValue}"></span>
            </span>
            <span class="category-list-money">ï¿¥{{item.barValue | number:'1.2-2'}}</span>
        </li>

    </ul>

</div>